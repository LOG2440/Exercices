{
  "name": "tictactoe",
  "version": "1.0.0",
  "description": "Exercices sur les tests avec JestJS",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "http-server -a localhost -p 3000 -o src",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --env=jsdom --verbose",
    "coverage": "npm test -- --coverage"
  },
  "keywords": [
    "Jest",
    "tests",
    "validation"
  ],
  "author": "",
  "license": "CC-BY-3.0",
  "devDependencies": {
    "http-server": "^14.1.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/*.js"
    ]
  }
}